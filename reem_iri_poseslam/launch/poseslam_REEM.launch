<!-- poseslam testing-->
<launch>

<param name="use_sim_time" value="true" />

<node name="rviz"
      pkg="rviz"
      type="rviz"
      args="-d $(find iri_poseslam)/config/poseslam.vcg" />

<node name="trajectory_scans_2_pointcloud"
      pkg="iri_poseslam"
      type="trajectory_scans_2_pointcloud"
      output="screen">
      <remap from="~scan" to="/scan_filtered" /> <!--/teo/front_laser/scan - /teo/sensors/front_laser-->
      <remap from="~trajectory" to="/iri_poseslam/trajectory" />
</node>

<node name="trajectory_scans_2_pointcloud_no_loops"
      pkg="iri_poseslam"
      type="trajectory_scans_2_pointcloud"
      >
      <remap from="~scan" to="/scan_filtered" /> <!--/teo/sensors/front_laser-->
      <remap from="~trajectory" to="/iri_poseslam/no_loops_trajectory" />
</node>

<node name="scans_2_odom"
      pkg="iri_poseslam"
      type="scans_2_odom"
      output="screen">
      <remap from="~scan" to="/scan_filtered" /> <!--/teo/sensors/front_laser-->
      <remap from="~get_relative_pose" to="/iri_laser_icp/get_relative_pose" />
      <param name="~online_mode" value="True"/>
      <param name="~bad_cov_thres" value="1.0e-3"/>
      <param name="~ICP_covariance_correction_factor" value="10.0"/>
      
</node>

<node name="trajectory_to_markers"
      pkg="iri_poseslam"
      type="trajectory_to_markers"
      output="screen">
      <remap from="~trajectory" to="/iri_poseslam/trajectory" />
      <param name="~marker_color_r" value="0.6"/>
      <param name="~marker_color_g" value="0.2"/>
      <param name="~marker_color_b" value="0.2"/>
      <param name="~marker_color_a" value="1"/>
      <param name="~actual_marker_color_r" value="0.8"/>
      <param name="~actual_marker_color_g" value="0.4"/>
      <param name="~actual_marker_color_b" value="0.4"/>
      <param name="~actual_marker_color_a" value="1"/>
      <param name="~marker_loop_color_r" value="0.4"/>
      <param name="~marker_loop_color_g" value="0.8"/>
      <param name="~marker_loop_color_b" value="0.0"/>
      <param name="~marker_loop_color_a" value="1"/>
      <param name="~line_loop_color_r" value="0.4"/>
      <param name="~line_loop_color_g" value="0.8"/>
      <param name="~line_loop_color_b" value="0.0"/>
      <param name="~line_loop_color_a" value="0.8"/>
      <param name="~line_loop_width" value="0.05"/>
      <param name="~line_color_r" value="0.6"/>
      <param name="~line_color_g" value="0.2"/>
      <param name="~line_color_b" value="0.2"/>
      <param name="~line_color_a" value="1"/>
      <param name="~line_width" value="0.02"/>
</node>

<node name="trajectory_no_loops_to_markers"
      pkg="iri_poseslam"
      type="trajectory_to_markers"
      >
      <remap from="~trajectory" to="/iri_poseslam/no_loops_trajectory" />
      <param name="~marker_color_r" value="1"/>
      <param name="~marker_color_g" value="1"/>
      <param name="~marker_color_b" value="1"/>
      <param name="~marker_color_a" value="0.3"/>
      <param name="~line_color_r" value="1"/>
      <param name="~line_color_g" value="1"/>
      <param name="~line_color_b" value="1"/>
      <param name="~line_color_a" value="0.3"/>
      <param name="~line_width" value="0.01"/>
      <param name="~node_name" value="trajectory_no_loops"/>
</node>

<node name="iri_laser_icp"
      pkg="iri_laser_icp"
      type="iri_laser_icp"
      output="screen">
    <param name="~max_angular_correction_deg" value="50"/>
    <param name="~max_linear_correction" value="3.0"/>
    <param name="~max_iterations" value="50"/>
    <param name="~max_correspondence_dist" value="3.0"/>
    <param name="~clustering_threshold" value="0.1"/>
    <param name="~orientation_neighbourhood" value="200"/>
    <param name="~use_point_to_line_distance" value="0"/>
    <param name="~use_corr_tricks" value="1"/>
    <param name="~restart" value="1"/>
    <param name="~do_visibility_test" value="0"/>
    <param name="~do_compute_covariance" value="1"/>
    <param name="~epsilon_xy" value="0.000000001"/>
    <param name="~epsilon_theta" value="0.000000001"/>
    <param name="~debug_verify_tricks" value="0"/>
    <param name="~outliers_adaptive_order" value="0.8"/>
</node>

